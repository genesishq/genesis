{
  "name": "genesis",
  "version": "1.0.0",
  "description": "Boilerplate for advanced web apps with webpack, babel and lots of goodies.",
  "author": "Magnus Bergman <hello@magnus.sexy>",
  "engines": {
    "node": "5.3.0",
    "npm": "3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:genesishq/genesis.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "scripts": {
    "postinstall": "npm run build",
    "test": "npm run lint & cross-env NODE_ENV=test karma start",
    "lint": "standard",
    "clean": "rimraf public",
    "start": "cross-env NODE_ENV=production node server",
    "dev": "cross-env NODE_ENV=development nodemon server -w server",
    "prebuild": "npm run clean -s",
    "build": "cross-env NODE_ENV=production webpack -p --colors --progress"
  },
  "standard": {
    "parser": "babel-eslint"
  },
