  "engines": {
    "node": "0.10.36",
    "npm": "2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:magnus-bergman/genesis.git"
  },
  "scripts": {
    "start": "npm run build && node server",
    "test": "./node_modules/.bin/karma start karma.conf.js --log-level debug --single-run",
    "hot": "./node_modules/.bin/webpack-dev-server --progress --colors --hot --inline",
    "build": "./node_modules/.bin/rimraf ./public/assets ./public/tmp ./public/index.html && NODE_ENV=production ./node_modules/.bin/webpack -p --colors --progress --config webpack.production.config.js"
  },
